{
    "manifest_version": 3,
    "name": "turbo fuck",
    "version": "1.5",
    "description": "Расширение для turbo tap со сном.",
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "*://*/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://tap.eclipse.xyz/*"],
            "js": ["content.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    }
}
